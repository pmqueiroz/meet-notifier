  "scripts": {
    "dev": "electron .",
    "test": "node src/utils/test.sync.js",
    "pub": "git add . && git-cz",
    "commit": "git-cz"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "clipboardy": "^2.3.0",
    "dotenv": "^8.2.0",
    "googleapis": "59.0.0",
    "install": "^0.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "electron": "^10.1.2",
    "electron-wix-msi": "^3.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
