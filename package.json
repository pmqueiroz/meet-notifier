{
  "name": "meet-notifier",
  "description": "A Google Meet notifier for meets schedules in calendar.google",
  "version": "0.0.2",
  "main": "src/index.js",
  "repository": "https://github.com/pmqueiroz/meet-notifier.git",
  "author": "pmqueiroz <pedroqueiroz593@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
